export GUIX_LOCPATH="$HOME/.guix-profile/lib/locale${GUIX_LOCPATH:+:}$GUIX_LOCPATH"
export TERMINFO_DIRS="$HOME/.guix-profile/share/terminfo${TERMINFO_DIRS:+:}$TERMINFO_DIRS"
